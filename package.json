        "nprogress": "^0.2.0",
        "sweetalert2": "^11.4.8",
        "v3-infinite-loading": "^1.0.6",
        "vue": "^3.2.33",
        "vue-chartjs": "^4.0.7",
        "vue-i18n": "^9.2.0-beta.35",
