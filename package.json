{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "dev-user": "npm --section=users run dev",
        "dev-admin": "npm --section=admin run dev",
        "watch-user": "npm --section=users run watch",
        "watch-admin": "npm --section=admin run watch",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm --section=users run production && npm --section=admin run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "axios": "^0.21",
        "browser-sync": "^2.27.9",
        "browser-sync-webpack-plugin": "^2.3.0",
